@model IEnumerable<emsproject23.Models.Credentials>

@{
    ViewData["JobTitle"] = "Index";
}

<h1>Index</h1>

<p>
   
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EncPass)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EncPass)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" class="alert-warning">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id" class="alert-info">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id" class="alert-danger">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
